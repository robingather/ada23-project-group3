{
  "$schema": "http://www.krakend.io/schema/v3.json",
  "version": 3,
  "output_encoding": "json",
  "name": "2929_API",
  "port": 8081,
  "endpoints": [
    {
      "endpoint": "/account_tickets",
      "method": "GET",
      "timeout": "800ms",
      "output_encoding": "json",
      "input_headers": [
        "Authorization"
      ],
      "extra_config": {
        "qos/ratelimit/router": {
          "max_rate": 3,
          "client_max_rate": 0,
          "strategy": "ip",
          "capacity": 0,
          "client_capacity": 0
        }
      },
      "backend": [
        {
          "url_pattern": "/tickets",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://secure_api:5000/"
          ]
        },
        {
          "url_pattern": "/verify",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://secure_api:5000/"
          ]
        }
      ]
      
    }
  ]
}