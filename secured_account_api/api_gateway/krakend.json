{
  "$schema": "http://www.krakend.io/schema/v3.json",
  "version": 3,
  "output_encoding": "json",
  "name": "2929_API",
  "port": 8081,
  "endpoints": [
    {
      "endpoint": "/account_tickets",
      "method": "GET",
      "output_encoding": "json",
      "input_headers": [
        "Authorization"
      ],
      "extra_config": {
        "proxy": {
          "sequential": true
        }
      },
      "backend": [
        {
          "url_pattern": "/tickets",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://secure_api:5000/"
          ],
          "disable_host_sanitize": false
        },
        {
          "url_pattern": "/accounts",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://secure_api:5000/"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}